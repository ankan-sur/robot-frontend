###############################################
# Network endpoints
# All of these fall back to window.location.hostname
# so you can often leave them unset. Override when you
# reverse-proxy or tunnel the services.
###############################################

# ROS Bridge WebSocket connection (ws://HOST:9090 by default)
VITE_ROSBRIDGE_URL=

# Video server base URL (http://HOST:8080 by default)
VITE_VIDEO_BASE=

# visualization_rwt base url (http://HOST:8001 by default)
VITE_RWT_BASE=

# Individual widget URLs (override when serving widgets separately)
VITE_RWT_MAP_URL=
VITE_RWT_TELEOP_URL=
VITE_RWT_IMAGE_URL=

# Optional external tooling
VITE_ROS_TOOL_URL=
VITE_ROSBOARD_URL=

# Optional: Backend API (if you still need it)
VITE_API_BASE=http://fordward.local/api

# Optional: Control token for robot command authentication
# Only needed if CONTROL_TOKEN is set on the backend
# VITE_CONTROL_TOKEN=your_secret_token_here
